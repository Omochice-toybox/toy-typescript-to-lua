{
  "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/TypeScriptToLua/master/tsconfig-schema.json",
  "extends": "@tsconfig/strictest/tsconfig.json",
  "include": [
    "src",
  ],
  "compilerOptions": {
    "target": "ESNext",
    "outDir": "dist",
    "lib": [
      "ESNext",
      "dom"
    ],
    "moduleResolution": "Node",
    "types": []
  },
  "tstl": {
    "luaTarget": "JIT"
  }
}
